<?xml version="1.0" encoding="utf-8"?>
<form>
  <title>Team Style 1</title>
	<sampledata></sampledata>
	<fields name="team-intro">
		<fieldset name="team-basic" label="TEAM_BASIC_LABEL" description="TEAM_BASIC_DESC">
			<field name="team-heading" type="text" default="" label="TEAM_HEADING_LABEL" description="TEAM_HEADING_DESC" />
			<field name="team-desc" type="textarea" default="" label="TEAM_DESC_LABEL" description="TEAM_DESC_DESC" />

			<field name="columns" type="list" label="TEAM_COLUMNS_LABEL" description="TEAM_COLUMNS_DESC" >
				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
			</field>
		</fieldset>

		<fieldset name="section-team" label="TEAM_SECTION_LABEL_LABEL" description="TEAM_SECTION_LABEL_DESC">
			<field name="data" type="jalist" >
				<item name="image" type="media" default="" label="TEAM_IMAGE_LABEL" description="TEAM_IMAGE_DESC" />
				<item name="title" type="text" default="" label="TEAM_TITLE_LABEL" description="TEAM_TITLE_DESC" />
				<item name="position" type="text" default="" label="TEAM_POSITION_LABEL" description="TEAM_POSITION_DESC" />
				<item name="desc-info" type="textarea" default="" label="TEAM_DESC_INFO_LABEL" description="TEAM_DESC_INFO_DESC" />
			</field>
		</fieldset>
  </fields>
</form>